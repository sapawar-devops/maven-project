pipeline {
    agent any
    stages {
        stage('clone the code from git repo'){
          git 'https://github.com/sapawar-devops/maven-project'
          }
    }  
    {
         stage ('Package creation'){
        
            steps {
                withMaven(maven : 'LocalMaven') {
                    sh 'mvn package'
                 }
              }
         }
      }
  
}
